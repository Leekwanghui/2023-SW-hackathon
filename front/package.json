{
  "name": "GoodNews",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "pretty": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint src --quiet",
    "lint:css": "stylelint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint ./src --ext \".js,.jsx,.ts,.tsx\" --fix && stylelint \"./src/**/*.css\" --fix",
    "apk": "react-native run-android --variant=release",
    "apk-install": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "apk-uninstall": "adb uninstall com.hellotemp",
    "bundle": "cd android && ./gradlew bundleRelease",
    "ios-project": "open ios/HelloTemp.xcodeproj",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/stack": "^6.3.12",
    "axios": "^1.4.0",
    "http-proxy-middleware": "^2.0.6",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-base64": "^0.2.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-reanimated-carousel": "^3.4.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/classnames": "^2.3.1",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.10",
    "@types/react-native": "^0.71.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsc": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.1.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "stylelint": "^15.0.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "proxy": "http://localhost:8080"
}
